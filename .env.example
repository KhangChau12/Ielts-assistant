# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI (for vocabulary generation with GPT-4o)
OPENAI_API_KEY=your_openai_api_key

# Groq API Keys (for essay scoring/improvement - supports multiple keys for rate limit rotation)
# Add as many keys as you want: GROQ_API_KEY_1, GROQ_API_KEY_2, etc.
GROQ_API_KEY_1=your_first_groq_api_key
GROQ_API_KEY_2=your_second_groq_api_key
GROQ_API_KEY_3=your_third_groq_api_key
# ... add more as needed

# Upstash Redis (for rate limiting - optional but recommended for production)
# Sign up at https://upstash.com/ and create a Redis database
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
